;不懂的就别动了
API="127.0.0.1:5700"
token=
;命令前缀，可以设置俩
prefix="#"
prefix2=";"
enablePrefix2=true
;依次填入你的Q号、你机器人的Q号和你的开发群
master=
bot=
devgroup=
;是否允许加好友和加群，可选 true false ignore
allowFriends=false
allowGroups=ignore
;添加好友或新人进群时的默认提示语
addFriendMsg="BLBot 已经是你的好友啦，发送 #help 查看帮助～"
addGroupMsg="欢迎加入本群，我是 BLBot，发送 #help 查看帮助～"
;调试选项
DEBUG=false
DebugListen=
Listen=-1
;是否开启一些功能
recordStat=false
alias=true
;这里往下是模块定义的 config，以后可能会改为使用 data
;填入cookie和一些key，看着填就可以了
osu_cookie=
osu_api_key=
baidumap_ak_key=
amap_api_key=
voice_app_id=
voice_api_key=
voice_secret_key=
translate_appkey=
translate_secret_key=
